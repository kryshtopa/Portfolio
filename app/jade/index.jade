body
  .wrapper
    header.site-header
      .container
        .site-header__social
          ul.social__list
            each val, index in {facebook: "http://", "twitter": "http://", "github": "http://"}
              li.social__item
                a.social__link(href="" + val class="social__link_" + index + " " + "sprite-social " + "sprite-social-" + index,  target="_blank")= index
        .site-header__logo
          img.logo_img(src="images/logo.png" alt="Dmitry Kryshtopa logo")
        .site-header__greeting
