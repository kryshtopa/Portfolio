    .slide-in.slide-in_message
      a.slide-in__content.slide-in__content_message send me a message
    .slide-in.slide-in_colors
      .slide-in__content.slide-in__content_colors
        ul.color-switch__list
          -var colors = ["orange", "lightblue", "black"]
          each color in colors
            li.color-switch__item(class="color-switch__item_" + color + ((color == "lightblue") ? " active" : "") style="background-color:" + color)
    .slide-in.slide-in_text-size
